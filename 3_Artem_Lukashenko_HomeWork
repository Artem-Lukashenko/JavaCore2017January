package com.company;

public class Main {

    public static void main(String[] args) {
        // write your code here
        for (int i=10; i > 0; i--){
            System.out.println(i + " негритят сидело за столом");
            System.out.println("1 негритенок вышел из-за стола");
        }
        System.out.println("За столом не осталось негритят");
    }
}
