public class Main {
  public static void main(String[] args) {
    int whitecell=9989;
    int blackcell=9724;
    char ch = '\n';
    for (int i=1;i<=8;i++){
           for (int j=0;j<8;j++){   
              if (j%2 == 0 )
              System.out.print((char)blackcell);
              else
              System.out.print((char)whitecell);
           }
              System.out.print(ch);
    }
  }
}
